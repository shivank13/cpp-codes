#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int n,m;
	cin>>n>>m;
	int a[n],b[m];
	for(int i=0;i<n;i++)
	cin>>a[i];
	for(int i=0;i<m;i++)
	cin>>b[i];
	int i=0;
	while(a[n-1]>b[0])
	{
	    if(a[i]>b[0])
	    {swap(a[i],b[0]);
	    sort(b,b+m);}
	    i++;
	    
	    
	}
	for(int i=0;i<n;i++)
	cout<<a[i]<<" ";
	cout<<"\n";
	for(int i=0;i<m;i++)
	cout<<b[i]<<" ";
	return 0;
}
